{
  "search.exclude": {
    "meta/logs/2*/2*/": true,
    "packages/markout": true,
    "packages/markie": true,
    "components": false,
    "console": false,
    "esm": false,
    "experimental": false,
    "helpers": false,
    "markup": false,
    "pholio": false,
    "quench": false,
    "**/node_modules": true,
    "**/bower_components": true,
    "**/archive": true,
    "**/trash": true,
    "**/dist": true,
    "**/[more]": true,
    "**/[trash]": true,
    "**/.git": true
  },
  "search.useIgnoreFiles": true,
  "files.watcherExclude": {
    "packages/markout": true,
    "components": false,
    "console": false,
    "esm": false,
    "experimental": false,
    "helpers": false,
    "markup": false,
    "pholio": false,
    "quench": false,
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules": true,
    "**/bower_components": true,
    "**/archive": true,
    "**/trash": true,
    "**/dist": true,
    "**/[more]": true
  },
  "files.exclude": {},
  "explorer.decorations.colors": false,
  "git.autoRepositoryDetection": true,
  "git.scanRepositories": ["console", "components", "esm", "experimental", "markout", "markup", "pholio", "quench"],
  "cSpell.words": ["interop"],
  "cSpell.enabled": true,
  "cSpell.enabledLanguageIds": ["html", "markdown"],
  "cSpell.showStatus": true,
  "cSpell.language": "en",
  "javascript.implicitProjectConfig.checkJs": true,
  "path-intellisense.mappings": {
    "/": "${workspaceRoot}/"
  },
  "files.associations": {
    "*.mo": "markdown",
    "*.mout": "markdown",
    "*.markout": "markdown"
  },
  "markdown.extension.showExplorer": true,
  "markdown.preview.doubleClickToSwitchToEditor": true,
  "markdown.styles": [".vscode.markdown.css"],
  // "prettier.configPath": "${workspaceRoot}/packages/tools/prettier.config/.prettierrc.json",
  "editor.tabSize": 2,
  "npm.packageManager": "yarn",
  "npm.scriptExplorerAction": "run",
  "npm.enableRunFromFolder": true,
  "npm.autoDetect": "on",
  "terminal.integrated.experimentalUseTitleEvent": true,
  "terminal.integrated.experimentalRefreshOnResume": true,
  "workbench.view.alwaysShowHeaderActions": false,
  "workspace.exclude": {
    "components": false,
    "console": false,
    "esm": false,
    "experimental": false,
    "helpers": false,
    "markup": false,
    "pholio": false,
    "quench": false
  }
}
