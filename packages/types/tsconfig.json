{
  "compilerOptions": {
    "composite": true,
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "typeRoots": [],
    "forceConsistentCasingInFileNames": true,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "noResolve": false,
    "diagnostics": false,
    "traceResolution": false,
    "listEmittedFiles": true,
    "listFiles": true,
    "pretty": true,
    "noEmit": true
  },
  "files": ["./index.d.ts"],
  "exclude": ["node_modules", "**/node_modules/*", "trash", "**/trash/*"],
  "references": [{"path": "./common"}, {"path": "./browser"}]
}
