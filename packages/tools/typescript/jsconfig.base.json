{
  "$schema": "http://json.schemastore.org/jsconfig",
  "compilerOptions": {
    "moduleResolution": "classic",
    "module": "esnext",
    "target": "esnext",
    "keyofStringsOnly": false,
    "noEmit": true,
    "forceConsistentCasingInFileNames": true,
    "strictNullChecks": false,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules",
    "**/node_modules/*",
    "trash",
    "**/trash/*",
    "[[]trash[]]",
    "**/[[]trash[]]/*",
    "[[]more[]]",
    "**/[[]more[]]/*",
    "[trash]",
    "**/[trash]/*",
    "[more]",
    "**/[more]/*",
    "@*"
  ]
}
