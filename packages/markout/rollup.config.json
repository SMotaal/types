{
  "defaults": {
    "cache": false,
    "context": "this",
    "output": {"sourcemap": true, "interop": false}
  },
  "scopes": {
    "/markout/": "./",
    "/markup/": "../markup/",
    "/components/": "../components/"
  },
  "bundles": [
    {
      "input": {
        "markup": "./lib/markup.js",
        "markout": "./lib/markout.js",
        "components": "./lib/components.js",
        "browser": "./lib/browser.js"
      },
      "output": {
        "format": "esm",
        "exports": "named",
        "chunkFileNames": "[name].js",
        "dir": "./dist/",
        "paths": {
          "/markout/lib/helpers.js": "../lib/helpers.js",
          "/markup/dist/tokenizer.browser.js": "../../markup/dist/tokenizer.browser.js",
          "/browser/legacy.js": "../../../browser/legacy.js"
        }
      },
      "external": ["/browser/legacy.js"],
      "chunks": "scopes"
    }
  ]
}
